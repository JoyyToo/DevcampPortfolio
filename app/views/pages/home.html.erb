<h1>HomePage</h1>

<h1>Blog Posts</h1>
<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th colspan="4"></th>
  </tr>
  </thead>

  <tbody>
  <% @posts.each do |blog| %>
    <tr>
      <td><%= blog.title.upcase %></td>
      <td><%= blog.body %></td>
    </tr>
  </tbody>
<% end %>
</table>
